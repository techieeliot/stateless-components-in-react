function tick() {
  const element = (
    <div>
      <h1>w3develops was here</h1>
      <h2>It is {new Date().getHours()}.</h2>
      <p>The minute is {new Date().getMinutes()}</p>
      <p>The seconds are {new Date().getSeconds()}</p>
      {/*.toLocaleTimeString()*/}
    </div>
  );
  ReactDOM.render(
    element,
    document.getElementById('root')
  );
}

setInterval(tick, 1000);
